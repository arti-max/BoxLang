; reserve 4k for kernel or other programm at 00 on disk
num16 void_fill : ?4096
lib disk : incl <disk>
lib io : incl <io>

char boot_text: {'G' 'A' 'B' 'o' 'o' 't' ' ' 'v' '0' '.' '1' '$'}

box boot[] (
    io->print[boot_text]
    disk->read_sector[0, $0000]
    disk->read_sector[1, $0200]
    gasm["jmp $0000"]
)